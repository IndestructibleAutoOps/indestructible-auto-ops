{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "indestructibleautoops.event.schema.json",
  "type": "object",
  "required": ["ts", "traceId", "stepId", "type", "payload"],
  "properties": {
    "ts": { "type": "string", "minLength": 10 },
    "traceId": { "type": "string", "minLength": 8 },
    "stepId": { "type": "string", "minLength": 1 },
    "type": { "type": "string", "minLength": 1 },
    "payload": { "type": "object" }
  }
}