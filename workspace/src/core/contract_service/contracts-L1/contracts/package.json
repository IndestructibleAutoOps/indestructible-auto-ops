{
  "name": "@machinenativeops/contracts-l1",
  "version": "1.0.0",
  "description": "SynergyMesh Contracts L1 - Core contract management service",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "build:clean": "npm run clean && npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint": "eslint 'src/**/*.ts' --max-warnings 0 --ignore-pattern '**/*.test.ts' --ignore-pattern '**/__tests__/**'",
    "lint:fix": "eslint src/**/*.ts --fix --ignore-pattern \"**/*.test.ts\" --ignore-pattern \"**/__tests__/**\"",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:self-healing": "jest --testPathPattern=self-healing --passWithNoTests",
    "examples:self-healing": "tsx examples/self-healing/run-all-examples.ts",
    "example:basic": "tsx examples/self-healing/01-basic-usage.ts",
    "example:config": "tsx examples/self-healing/02-custom-configuration.ts",
    "example:monitoring": "tsx examples/self-healing/03-monitoring-governance.ts",
    "example:events": "tsx examples/self-healing/04-event-subscription.ts",
    "example:demo": "tsx examples/self-healing/05-complete-demo.ts",
    "typecheck": "tsc --noEmit",
    "sbom": "npm audit --audit-level moderate --json > sbom.json",
    "scan": "npm audit --audit-level high",
    "governance:self-healing-report": "node -e \"const g = require('./dist/governance/self-healing-integration'); console.log(g.selfHealingGovernance.exportGovernanceReport());\" || echo 'Build dist first with: npm run build'",
    "predeploy": "npm run typecheck && npm run format:check && npm run lint && npm run test && npm run build",
    "clean": "rimraf dist"
  },
  "keywords": [
    "contracts",
    "l1",
    "machinenativeops",
    "microservice"
  ],
  "author": "SynergyMesh Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@sigstore/sign": "^4.1.0",
    "@sigstore/verify": "^3.1.0",
    "cors": "^2.8.6",
    "crypto": "^1.0.1",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.2",
    "rate-limit-redis": "^4.3.1",
    "sanitize-filename": "^1.6.3",
    "uuid": "^13.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.10",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.5",
    "jest": "^30.2.0",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.2",
    "supertest": "^7.2.2",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3"
  }
}
