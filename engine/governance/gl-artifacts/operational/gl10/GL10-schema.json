{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "GL10 Artifact Schema",
  "type": "object",
  "required": [
    "id",
    "gllevel",
    "name",
    "createdat",
    "immutable"
  ],
  "properties": {
    "id": {
      "type": "string"
    },
    "gllevel": {
      "type": "string",
      "pattern": "^GL1[0-9]$"
    },
    "name": {
      "type": "string"
    },
    "createdat": {
      "type": "string",
      "format": "date-time"
    },
    "immutable": {
      "type": "boolean"
    }
  },
  "_GL": {
    "governed": true,
    "layer": "governance",
    "semantic": "GL10-schema",
    "audit_trail": "../../engine/governance/GL_SEMANTIC_ANCHOR.json",
    "charter": "GL Unified Charter",
    "version": "2.0.0"
  }
}
