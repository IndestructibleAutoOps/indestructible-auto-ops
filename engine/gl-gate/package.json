{
  "name": "@machine-native-ops/gl-gate",
  "version": "1.0.0",
  "description": "GL-Gate Governance Gate System - Comprehensive governance gates for machine-native-ops engine",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./gates": {
      "types": "./dist/gates/index.d.ts",
      "require": "./dist/gates/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/types/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "validate-gates": "node scripts/validate-gates-01-99.js",
    "validate-gates:strict": "node scripts/validate-gates-01-99.js --strict --no-continue-on-error"
  },
  "keywords": [
    "governance",
    "gate",
    "quality-gate",
    "ci-cd",
    "compliance",
    "security",
    "observability",
    "testing",
    "validation",
    "machine-native",
    "gl-gate"
  ],
  "author": "MachineNativeOps",
  "license": "MIT",
  "dependencies": {
    "ajv": "^8.12.0",
    "js-yaml": "^4.1.0",
    "winston": "^3.11.0",
    "prom-client": "^15.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
